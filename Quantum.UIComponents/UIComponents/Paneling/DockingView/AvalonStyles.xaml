    <DataTemplate x:Key="AvalonDock_Expression_DocumentHeaderTemplate">
        <TextBlock Text="{Binding Title}" TextTrimming="CharacterEllipsis" />
    </DataTemplate>
    <DataTemplate x:Key="AvalonDock_Expression_AnchorableHeaderTemplate">
        <Grid p:AvalonProperties.IsEventTarget="True">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition />
            </Grid.ColumnDefinitions>
            <Image
                x:Name="icon"
                Source="{Binding IconSource, Converter={StaticResource NullToDoNothingConverter}}"
                Stretch="Uniform" />
            <TextBlock
                x:Name="title"
                Grid.Column="1"
                Margin="4,0,0,0"
                Text="{Binding Title}"
                TextTrimming="CharacterEllipsis"
                ToolTip="{Binding Title}">
                <TextBlock.Foreground>
                    <SolidColorBrush Color="Gainsboro" />
                </TextBlock.Foreground>
            </TextBlock>
        </Grid>
        <DataTemplate.Triggers>
            <DataTrigger Binding="{Binding IconSource}" Value="{x:Null}">
                <Setter TargetName="title" Property="Margin" Value="0" />
            </DataTrigger>
            <EventTrigger RoutedEvent="p:AvalonProperties.UserNotifyBubble">
                <BeginStoryboard>
                    <Storyboard>
                        <ColorAnimation
                            AutoReverse="True"
                            DecelerationRatio="1"
                            RepeatBehavior="1x"
                            Storyboard.TargetName="title"
                            Storyboard.TargetProperty="Foreground.Color"
                            From="Gainsboro"
                            To="Gray"
                            Duration="0:0:0.3" />
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>
            <EventTrigger RoutedEvent="p:AvalonProperties.UserNotifyWarningBubble">
                <BeginStoryboard>
                    <Storyboard>
                        <ColorAnimation
                            AutoReverse="False"
                            DecelerationRatio="1"
                            RepeatBehavior="1x"
                            Storyboard.TargetName="title"
                            Storyboard.TargetProperty="Foreground.Color"
                            From="Gainsboro"
                            To="Yellow"
                            Duration="0:0:0.5" />
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>

            <EventTrigger RoutedEvent="p:AvalonProperties.UserNotifyErrorBubble">
                <BeginStoryboard>
                    <Storyboard>
                        <ColorAnimation
                            AutoReverse="False"
                            DecelerationRatio="1"
                            RepeatBehavior="1x"
                            Storyboard.TargetName="title"
                            Storyboard.TargetProperty="Foreground.Color"
                            From="Gainsboro"
                            To="Red"
                            Duration="0:0:1.4" />
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>

            <EventTrigger RoutedEvent="p:AvalonProperties.UserNotifyClearNotifications">
                <BeginStoryboard>
                    <Storyboard>
                        <ColorAnimation
                            AutoReverse="False"
                            DecelerationRatio="1"
                            RepeatBehavior="1x"
                            Storyboard.TargetName="title"
                            Storyboard.TargetProperty="Foreground.Color"
                            From="Gainsboro"
                            To="Gainsboro"
                            Duration="0:0:0.1" />
                    </Storyboard>
                </BeginStoryboard>
            </EventTrigger>
        </DataTemplate.Triggers>
    </DataTemplate>
